set xrange[-1:1]
set yrange[-0.5:0.5]
set view map
unset surface
unset key
set hidden3d
set contour base
set size 0.8
set size ratio 0.5
set xlab 'x'
set ylab 'y'
maxdata = 4.0E-03
mindata = 0.0

set term postscript eps monochrome solid enhanced

d = 2.0E-4
set cntrparam levels 50
set cntrparam levels incremental mindata, d, maxdata

set output '1d_20contour.eps'
splot 'bfza0021_1DRoe_MC_rewrite' w l

set output 'Multid_20contour.eps'
splot 'bfza0021_MultidHLLD_modes2_MC_rewrite' w l


